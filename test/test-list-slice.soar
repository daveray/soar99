source "../common.soar"

sp {test-list-slice*propose*slice
  (state <s> ^name test-list-slice
            -^result
             ^input <input>)
-->
  (<s> ^operator.name list*slice)
}

sp {test-list-slice*elaborate*slice-params
  (state <s> ^name test-list-slice
             ^operator <o>
             ^input.<attr> <value>)
  (<o> ^name list*slice)
-->
  (<o> ^<attr> <value>)
}

