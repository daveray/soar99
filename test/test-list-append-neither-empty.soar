source "test-list-append.soar"
sp {apply*init
  (state <s> ^operator.name init)
-->
  (<s> ^name test-list-append
       ^first (list d e f)
       ^second (list a b c))
}

sp {succeeded
  (state <s> ^name test-list-append
             ^result <result>)
  (<result> ^value d ^next <n0>)
  (<n0> ^value e ^next <n1>)
  (<n1> ^value f ^next <n2>)
  (<n2> ^value a ^next <n3>)
  (<n3> ^value b ^next <n4>)
  (<n4> ^value c ^next nil)
-->
  (write (crlf) |Succeeded|)
  (halt)
}
