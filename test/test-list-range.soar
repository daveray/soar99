setup {
  source "common.soar"
}

test list-range-with-no-start-argument {
  sp {apply*init
    (state <s> ^operator.name init)
  -->
    (<s> ^name test*list*range
        ^end 4)
  }

  sp {pass
    (state <s> ^name test*list*range
              ^result <result>)
    (<result> ^value 0 ^next <n0>)
    (<n0> ^value 1 ^next <n1>)
    (<n1> ^value 2 ^next <n2>)
    (<n2> ^value 3 ^next <n3>)
    (<n3> ^value 4 ^next nil)
  -->
    (pass)
  }

  sp {propose*list*range
    (state <s> ^name test*list*range
              ^end <end>
              -^result)
  -->
    (<s> ^operator <o>)
    (<o> ^name list*range
        ^end <end>)
  }
}

test list-range-with-start-and-end-arguments {
  sp {apply*init
    (state <s> ^operator.name init)
  -->
    (<s> ^name test*list*range
        ^start 10
        ^end 16
        ^step 2)
  }

  sp {pass
    (state <s> ^name test*list*range
              ^result <result>)
    (<result> ^value 10 ^next <n1>)
    (<n1> ^value 12 ^next <n2>)
    (<n2> ^value 14 ^next <n3>)
    (<n3> ^value 16 ^next nil)
  -->
    (pass)
  }

  sp {propose*list*range
    (state <s> ^name test*list*range
              ^end <end>
              ^start <start>
              ^step <step>
              -^result)
  -->
    (<s> ^operator <o>)
    (<o> ^name list*range
        ^end <end>
        ^start <start>
        ^end <end>
        ^step <step>)
  }
}

