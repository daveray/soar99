##!
# Common "functions"

sp {list*identity*apply
  "Identity operator"
  (state <s> ^operator <o>)
  (<o> ^name list*identity
       ^value <value>)
-->
  (<s> ^result <value>)
}
       
sp {list*multiply*apply
  "Identity operator"
  (state <s> ^operator <o>)
  (<o> ^name list*multiply
       ^factor <factor>
       ^value <value>)
-->
  (<s> ^result (* <factor> <value>))
}
       
